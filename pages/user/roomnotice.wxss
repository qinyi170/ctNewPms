page{background-color: #f2f2f2}
.scrollview{margin-top:10px}
.scrollview view.viewv{background-color: #fff;border-radius: 10rpx;font-size: 28rpx;padding:20rpx 40rpx;margin:0 40rpx 20rpx 40rpx;}
.scrollview view.viewv view{margin-bottom: 20rpx;}
.scrollview view.viewv view:last-of-type{line-height: 40rpx;}
.scrollview view.viewv text.text1{color: #313131;font-size: 32rpx}
.scrollview view.viewv text.text2{float: right;color: #888888}


