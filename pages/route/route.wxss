.container>view:first-of-type{margin-bottom: 30rpx;font-size: 30rpx;}
.container>view input{font-size: 30rpx;border-bottom: 1px solid #c2c2c2}
.container>view:last-of-type{margin-top: 50rpx;}
.imgwidth{width: 100%;display: block}
